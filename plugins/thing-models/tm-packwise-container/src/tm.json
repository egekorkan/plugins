            "address": {
              "title": "Company address",
              "type": "object",
              "properties": {
                "street": { "type": "string" },
                "city": { "type": "string" },
                "zip": { "type": "string" },
                "country": { "type": "string" }
              }
            },
            "meta": {
              "type": "object",
              "properties": {
                "apiKeyValue": { "type": "string" }
              }
            }
          }
        }
      },
      "readOnly": true,
      "writeOnly": false,
      "forms": [
        {
          "href": "/api/data",
          "contentType": "application/json",
          "op": ["readproperty"],
          "htv:methodName": "POST"
        }
      ]
    }
  }
}
